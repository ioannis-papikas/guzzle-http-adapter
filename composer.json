{
    "name": "ipapikas/guzzle-http-adapter",
    "description": "Http Adapter for Guzzle Client",
    "license": "MIT",
    "support": {
        "source": "https://github.com/ioannis-papikas/guzzle-http-adapter",
        "issues": "https://github.com/ioannis-papikas/guzzle-http-adapter/issues"
    },
    "authors": [
        {
            "name": "Ioannis Papikas",
            "email": "papikas.ioan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "HttpAdapter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HttpAdapter\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
